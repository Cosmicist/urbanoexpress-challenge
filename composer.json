{
    "require": {
        "doctrine/orm": "^3",
        "doctrine/dbal": "^4",
        "symfony/cache": "^7",
        "slim/slim": "4.*",
        "slim/psr7": "^1.8",
        "php-di/php-di": "^7.1",
        "vlucas/phpdotenv": "^5.6",
        "php-di/slim-bridge": "^3.4",
        "hidehalo/nanoid-php": "^2.0",
        "monolog/monolog": "^3.9",
        "ramsey/uuid-doctrine": "^2.1",
        "opis/json-schema": "^2.6"
    },
    "autoload": {
        "psr-4": {
          "": "src/",
          "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^4.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
